  "tailwindCSS.experimental.classRegex": [
    ["class\\s*=\\s*\"([^\"]*)", 1],
    ["class:\\s*\"([^\"]*)", 1]
  ],
  "CodeGPT.apiKey": "Google AI Studio"
}
